from kivymd.app import MDApp
from kivymd.uix.screen import MDScreen

class PesquisaScreen(MDScreen):
    def __init__(self, **kwargs):
        super().__init__(**kwargs)
        self.background_color = [1, 1, 1, 1]  # Exemplo de propriedade válida
        self.size_hint = (1, 1)  # Exemplo de propriedade válida

class MainApp(MDApp):
    def build(self):
        return PesquisaScreen()

if __name__ == '__main__':
    MainApp().run()